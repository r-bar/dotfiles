function fly-login() {
  if ! hash bw 2> /dev/null; then
    echo Bitwarden cli '`bw`' required
    exit 1
  fi
  creds="$(bw get item 462b30de-7062-4e46-88f8-aa27009774dc)"
  username="$(echo "$creds" | jq -r .login.username)"
  password="$(echo "$creds" | jq -r .login.password)"
  fly login -u "$username" -p "$password" $@
}
